<!ENTITY % DocBookDTD PUBLIC "-//OASIS//DTD DocBook XML V4.5//EN"
                             "docbkx45/docbookx.dtd">
%DocBookDTD;


<!-- ...................................................................... -->


<!ELEMENT standards (taxonomy, lists, records, community-of-interest, externalrefs?, organisations)>
<!ATTLIST standards
          revision CDATA #REQUIRED>

<!-- .......................... TAXONOMY .................................. -->

<!ELEMENT taxonomy (servicearea+)>

<!ELEMENT servicearea ((subarea | servicecategory)*)>
<!ATTLIST servicearea
          id ID #REQUIRED
          title CDATA #REQUIRED>

<!ELEMENT subarea ((servicecategory)*)>
<!ATTLIST subarea
          id ID #REQUIRED
          title CDATA #REQUIRED>

<!ELEMENT servicecategory ((category)*)>
<!ATTLIST servicecategory
          id ID #REQUIRED
          title CDATA #REQUIRED>

<!ELEMENT category ((subcategory)*)>
<!ATTLIST category
          id ID #REQUIRED
          title CDATA #REQUIRED>

<!ELEMENT subcategory EMPTY>
<!ATTLIST subcategory
          id ID #REQUIRED
          title CDATA #REQUIRED>


<!-- .......................... LISTS .................................. -->


<!ELEMENT lists ((profile-list | sp-list)*)>

<!ELEMENT profile-list (pf-view*)>
<!ATTLIST profile-list
          id ID #REQUIRED
          tag CDATA #REQUIRED
          tref IDREF #IMPLIED>


<!ELEMENT pf-view (parea?, (header?), pcatego? , pselect*, remarks?, rationale?)>
<!ATTLIST pf-view
          idx ID #IMPLIED
          category CDATA #IMPLIED>

<!ELEMENT parea (#PCDATA|para|indexterm)*>
<!ATTLIST parea
          svcarea IDREF #IMPLIED>

<!ELEMENT pcatego (#PCDATA|para|indexterm)*>
<!ATTLIST pcatego
          svcarea IDREF #IMPLIED>

<!ELEMENT protocol (#PCDATA|para|indexterm)*>

<!ELEMENT pselect (#PCDATA)>
<!ATTLIST pselect
          id IDREF #REQUIRED>


<!ELEMENT sp-list (sp-view*)>
<!ATTLIST sp-list
          tref IDREF #IMPLIED>

<!ELEMENT sp-view (ncoe?, (header?), select*, remarks?, rationale?)>
<!ATTLIST sp-view
          idx ID #IMPLIED
          category CDATA #IMPLIED>


<!ELEMENT select (#PCDATA)>
<!ATTLIST select
          id IDREF #REQUIRED
	        mode (unknown|mandatory|emerging|midterm|farterm|fading) #REQUIRED>

<!ELEMENT remarks (#PCDATA|para|indexterm)*>

<!ELEMENT rationale (#PCDATA | para | indexterm)*>


<!-- .......................... RECORDS .................................. -->


<!ELEMENT records ((standard | profile )*)>

<!ELEMENT standard (debug?, ((document | applicability | status)*))>
<!ATTLIST standard
          tag CDATA #REQUIRED
          id ID #REQUIRED
          ss (no | yes) "no"
          tref IDREF #IMPLIED>

<!ELEMENT profile (debug?, ((profilenote | profilespec | configuration | parts | applicability | status)*))>
<!ATTLIST profile
          type (base | coi-minor | coi) "base"
          tag CDATA #REQUIRED
          id ID #REQUIRED
          tref IDREF #IMPLIED>


<!ELEMENT profilespec EMPTY>
<!ATTLIST profilespec
          orgid  CDATA #IMPLIED
          pubnum CDATA #IMPLIED
          date   CDATA #IMPLIED
          title  CDATA #REQUIRED>


<!ELEMENT configuration (#PCDATA)>


<!ELEMENT debug (#PCDATA)>

<!ELEMENT document ((substandards | correction | alsoknown | comment)*)>
<!ATTLIST document
          orgid  CDATA #REQUIRED
          pubnum CDATA #REQUIRED
          date   CDATA #REQUIRED
          title  CDATA #REQUIRED>

<!ELEMENT substandards (refstandard+)>

<!ELEMENT correction (#PCDATA)>
<!ATTLIST correction
          cpubnum  CDATA #REQUIRED
          date     CDATA #REQUIRED>

<!ELEMENT alsoknown (#PCDATA)>
<!ATTLIST alsoknown
          orgid   CDATA #REQUIRED
          pubnum  CDATA #REQUIRED
          date    CDATA #REQUIRED>

<!ELEMENT comment (#PCDATA | ulink)*>

<!ELEMENT parts (refstandard|refprofile)+>


<!ELEMENT refstandard EMPTY>
<!ATTLIST refstandard
          refid IDREF #REQUIRED>

<!ELEMENT refprofile EMPTY>
<!ATTLIST refprofile
          refid IDREF #REQUIRED>


<!ELEMENT profilenote (#PCDATA)>





<!-- applicability content model identical to DocBook entry -->

<!ELEMENT applicability %ho; (%tbl.entry.mdl;)*>


<!ELEMENT status (info?, uri?, history)>
<!ATTLIST status
          mode   (unknown|rejected|retired) "unknown"
          stage  CDATA #REQUIRED>

<!ELEMENT info (#PCDATA | ulink)*>

<!ELEMENT history (event)+>

<!ELEMENT event (#PCDATA)>
<!ATTLIST event
          flag    (added|changed|deleted)   #REQUIRED
          date    CDATA   #REQUIRED
          rfcp    CDATA   #IMPLIED
          version CDATA   #REQUIRED>



<!ELEMENT externalrefs (exref*)>

<!ELEMENT exref EMPTY>

<!ATTLIST exref
          id     ID      #REQUIRED>


<!-- ................. COMMUNITY OF INTERESTS ............................ -->

<!ELEMENT community-of-interest (community*)>

<!ELEMENT community (profile*)>
<!ATTLIST community
          coi     CDATA      #REQUIRED>


<!-- ....................... ORGANISATION ................................ -->


<!ELEMENT organisations (orgkey*)>

<!ELEMENT orgkey EMPTY>
<!ATTLIST orgkey
          key    CDATA   #REQUIRED
          text   CDATA   #REQUIRED>



<!ELEMENT ncoe (#PCDATA)>

<!ELEMENT header EMPTY>



